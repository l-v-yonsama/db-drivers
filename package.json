{
  "name": "@l-v-yonsama/multi-platform-database-drivers",
  "version": "0.10.16",
  "description": "multi-platform database drivers in TypeScript.",
  "engines": {
    "node": ">= 18"
  },
  "devDependencies": {
    "@types/auth0": "^3.3.9",
    "@types/better-sqlite3": "^7.6.11",
    "@types/bytes": "^3.1.1",
    "@types/humanize-duration": "^3.27.2",
    "@types/jest": "~29.2",
    "@types/json-rules-engine": "^4.1.0",
    "@types/lodash.chunk": "^4.2.9",
    "@types/lodash.isequal": "^4.5.6",
    "@types/mssql": "^9.1.5",
    "@types/node": "~18",
    "@types/pg": "^8.6.6",
    "@types/pluralize": "^0.0.29",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "~5.44",
    "@typescript-eslint/parser": "~5.44",
    "eslint": "~8.28",
    "eslint-config-prettier": "~8.5",
    "eslint-plugin-jest": "~27.1",
    "jest": "~29.3",
    "prettier": "~2.8",
    "rimraf": "~3.0",
    "ts-jest": "~29.0",
    "tslib": "^2.6.3",
    "tsutils": "~3.21",
    "typedoc": "^0.25.1",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "~4.9"
  },
  "scripts": {
    "start": "node built/src/index.js",
    "clean": "rimraf coverage built tmp",
    "prebuild": "npm run lint",
    "doc": "typedoc --cleanOutputDir --excludePrivate --plugin typedoc-plugin-markdown --out doc src",
    "build": "npm run clean && tsc -p tsconfig.json",
    "build:watch": "npm run clean && tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json && npm run doc",
    "prepublishOnly": "npm run build:release",
    "lint": "eslint . --ext .ts --ext .mts",
    "lint:fix": "eslint --fix . --ext .ts --ext .mts",
    "examples:cw": "npm run build && node built/src/examples/cw.js",
    "examples:cfn": "npm run build && node built/src/examples/cfn.js",
    "examples:dynamo": "npm run build && node built/src/examples/dynamo.js",
    "examples:mysql": "npm run build && node built/src/examples/mysql.js",
    "examples:mysql2": "npm run build && node built/src/examples/mysql2.js",
    "examples:mssql": "npm run build && node built/src/examples/mssql.js",
    "examples:mssql2": "npm run build && node built/src/examples/mssql2.js",
    "examples:postgres": "npm run build && node built/src/examples/postgres.js",
    "examples:rules": "npm run build && node built/src/examples/rules.js",
    "examples:aws": "npm run build && node built/src/examples/aws.js",
    "examples:auth0": "npm run build && node built/src/examples/auth0.js",
    "examples:keycloak": "npm run build && node built/src/examples/keycloak.js",
    "test0": "jest --coverage",
    "test": "npm run clean && jest --runInBand",
    "test:util": "npm run clean && jest __tests__/util/util.test.ts",
    "test:driver:mysql": "npm run clean && jest __tests__/db/drivers/MySQLDriver.test.ts",
    "test:driver:mssql": "npm run clean && jest __tests__/db/drivers/SQLServerDriver.test.ts",
    "test:driver:postgres": "npm run clean && jest __tests__/db/drivers/PostgresDriver.test.ts",
    "test:driver:sqlite": "npm run clean && jest __tests__/db/drivers/SQLiteDriver.test.ts",
    "test:driver:redis": "npm run clean && jest __tests__/db/drivers/RedisDriver.test.ts",
    "test:driver:auth0": "npm run clean && jest __tests__/db/drivers/Auth0Driver.test.ts",
    "test:driver:keycloak": "npm run clean && jest __tests__/db/drivers/KeycloakDriver.test.ts",
    "test:driver:s3": "npm run clean && jest __tests__/db/drivers/AwsS3Driver.test.ts",
    "test:driver:sqs": "npm run clean && jest __tests__/db/drivers/AwsSQSDriver.test.ts",
    "test:driver:cw": "npm run clean && jest __tests__/db/drivers/AwsCloudwatchDriver.test.ts",
    "test:driver:minio": "npm run clean && jest __tests__/db/drivers/AwsS3Driver.test.ts",
    "test:helper:sql": "npm run clean && jest __tests__/helpers/SQLHelper.test.ts",
    "test:helper:res:mysql": "npm run clean && jest __tests__/helpers/ResourceHelper.mysql.test.ts",
    "test:helper:res:postgres": "npm run clean && jest __tests__/helpers/ResourceHelper.postgres.test.ts",
    "test:helper:engine": "npm run clean && jest __tests__/helpers/RuleEngine.test.ts",
    "test:helper:resolver": "npm run clean && jest __tests__/helpers/CodeResolver.test.ts",
    "prettier": "prettier --config .prettierrc --write .",
    "test:watch": "jest --watch"
  },
  "author": "l-v-yonsama <32474032+l-v-yonsama@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.828.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.306.0",
    "@aws-sdk/client-dynamodb": "^3.654.0",
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/client-ses": "^3.398.0",
    "@aws-sdk/client-sfn": "^3.840.0",
    "@aws-sdk/client-sqs": "^3.398.0",
    "@aws-sdk/credential-providers": "^3.279.0",
    "@aws-sdk/lib-dynamodb": "^3.656.0",
    "@aws-sdk/s3-request-presigner": "^3.427.0",
    "@l-v-yonsama/rdh": "^0.2.7",
    "auth0": "^3.7.1",
    "axios": "^1.6.0",
    "bytes": "^3.1.2",
    "csv-parse": "^5.5.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.3.1",
    "enum-values": "^1.2.1",
    "humanize-duration": "^3.30.0",
    "ioredis": "^5.3.1",
    "js-yaml": "^4.1.0",
    "json-rules-engine": "^6.5.0",
    "jwt-decode": "^4.0.0",
    "lodash.chunk": "^4.2.0",
    "mime-lite": "^1.0.3",
    "mqtt": "^5.13.1",
    "mssql": "^11.0.1",
    "mysql2": "^3.1.2",
    "openid-client": "^5.6.1",
    "pg": "^8.9.0",
    "pgsql-ast-parser": "^11.0.1",
    "pluralize": "^8.0.0",
    "short-unique-id": "^4.4.4",
    "sql.js": "^1.10.3"
  },
  "volta": {
    "node": "18.12.1"
  },
  "main": "built/src/index.js",
  "types": "built/src/index.d.ts",
  "files": [
    "built/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l-v-yonsama/db-drivers.git"
  },
  "keywords": [
    "database clients"
  ],
  "bugs": {
    "url": "https://github.com/l-v-yonsama/db-drivers/issues"
  },
  "homepage": "https://github.com/l-v-yonsama/db-drivers#readme"
}
